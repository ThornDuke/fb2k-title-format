{
  "name": "foobar2000-title-formatting-syntax",
  "displayName": "Foobar2000 Title Formatting Syntax",
  "description": "Syntax highlighting, snippets and commands for Foobar2000's title formatting.",
  "version": "1.5.7",
  "author": "Thorn Duke",
  "publisher": "ThornDuke",
  "icon": "resources/main.png",
  "galleryBanner": {
    "color": "#001b33",
    "theme": "dark"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Keymaps"
  ],
  "keywords": [
    "foobar2000",
    "title formatting",
    "syntax highlighting",
    "snippets"
  ],
  "main": "./src/extension.js",
  "activationEvents": [
    "onCommand:foobar2000-title-formatting-syntax.joinLinesSmart"
  ],
  "contributes": {
    "languages": [
      {
        "id": "fb2k",
        "aliases": [
          "Foobar2000 Title Formatting"
        ],
        "extensions": [
          ".f2k"
        ],
        "configuration": "./config/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fb2k",
        "scopeName": "source.f2k",
        "path": "./syntaxes/foobar2000.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "fb2k",
        "path": "./snippets/FB2k-title-formatting.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "foobar2000-title-formatting-syntax.joinLinesSmart",
        "title": "Join Lines"
      },
      {
        "command": "foobar2000-title-formatting-syntax.joinLinesNoComments",
        "title": "Join Lines No Comments"
      },
      {
        "command": "foobar2000-title-formatting-syntax.removeIndentation",
        "title": "Remove Indentation"
      }
    ],
    "keybindings": [
      {
        "command": "foobar2000-title-formatting-syntax.joinLinesSmart",
        "key": "cmd+k cmd+z",
        "mac": "cmd+k cmd+z",
        "win": "ctrl+k ctrl+z",
        "linux": "ctrl+k ctrl+z",
        "when": "editorTextFocus"
      },
      {
        "command": "foobar2000-title-formatting-syntax.joinLinesNoComments",
        "key": "cmd+k cmd+y",
        "mac": "cmd+k cmd+y",
        "win": "ctrl+k ctrl+y",
        "linux": "ctrl+k ctrl+y",
        "when": "editorTextFocus"
      },
      {
        "command": "foobar2000-title-formatting-syntax.removeIndentation",
        "key": "cmd+k cmd+h",
        "mac": "cmd+k cmd+h",
        "win": "ctrl+k ctrl+h",
        "linux": "ctrl+k ctrl+h",
        "when": "editorTextFocus"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ThornDuke/fb2k-title-format"
  },
  "bugs": {
    "url": "https://github.com/ThornDuke/fb2k-title-format/issues"
  },
  "homepage": "https://github.com/ThornDuke/fb2k-title-format#readme",
  "pricing": "Free",
  "scripts": {
    "prettier": "prettier --write \"**/*.{json,md}\"",
    "pack:prerelease": "unset npm_lifecycle_script && unset npm_config_prefix && source $NVM_DIR/nvm.sh && nvm use && mkdir -p build && vsce package --githubBranch master --pre-release --out build",
    "pack": "unset npm_lifecycle_script && unset npm_config_prefix && source $NVM_DIR/nvm.sh && nvm use && mkdir -p build && vsce package --out build",
    "publish:prerelease": "unset npm_lifecycle_script && unset npm_config_prefix && source $NVM_DIR/nvm.sh && nvm use && vsce publish --githubBranch master --pre-release",
    "publish": "unset npm_lifecycle_script && unset npm_config_prefix && source $NVM_DIR/nvm.sh && nvm use && vsce publish --githubBranch master",
    "publ": "npm run publish",
    "update": "node ./scripts/update-all.js"
  },
  "devDependencies": {
    "prettier": "^3.0.0"
  }
}
