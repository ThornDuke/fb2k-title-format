{
  "name": "foobar2000-title-formatting-syntax",
  "displayName": "Foobar2000 Title Formatting Syntax",
  "description": "Syntax highlighting and snippets for Foobar2000's title formatting.",
  "version": "1.1.13",
  "author": "Thorn Duke",
  "publisher": "ThornDuke",
  "icon": "resources/main.png",
  "galleryBanner": {
    "color": "#001b33",
    "theme": "dark"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "foobar2000",
    "title formatting",
    "syntax highlighting",
    "snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "fb2k",
        "aliases": [
          "Foobar2000 Title Formatting"
        ],
        "extensions": [
          ".f2k"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fb2k",
        "scopeName": "source.f2k",
        "path": "./syntaxes/foobar2000.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "fb2k",
        "path": "./snippets/foobar2000.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ThornDuke/fb2k-title-format"
  },
  "bugs": {
    "url": "https://github.com/ThornDuke/fb2k-title-format/issues"
  },
  "homepage": "https://github.com/ThornDuke/fb2k-title-format#readme",
  "pricing": "Free",
  "scripts": {
    "prettier": "prettier --write \"**/*.{json,md}\"",
    "publish:prerelease": "vsce publish --githubBranch master --pre-release",
    "publish": "vsce publish --githubBranch master",
    "pack:prerelease": "vsce package --githubBranch master --pre-release",
    "pack": "vsce package --githubBranch master"
  },
  "devDependencies": {
    "prettier": "^3.0.0"
  }
}
